<ayaxphpclass>
<make object="class"> 
	<build out_file="cestado_monitoreo.php" classname="cestado_monitoreo" classlabel="Estado de monitoreo">
		<parse in_file="monitoreo.sql" tablename="estados_de_monitoreos" />
	</build>
</make>

<make object="search_and_list"> 
	<build out_file="estado_monitoreo.php" classname="cestado_monitoreo" title="Estado de Monitoreo">
		<field name="id_estado_de_monitoreo" label="Estado" presentation="TEXT" isvisible="false" />
		<field name="color" label="Color" presentation="TEXT" />
		<field name="color_html" label="Color Hexa" presentation="TEXT" isvisible="false" />
		<field name="monitoreos_semanales" label="Numero" presentation="INT" />
		<field name="descripcion" label="Descripcion" presentation="TEXT"  />
		<searchfield name="color" />
		<searchfield name="color_html" />
		<searchfield name="monitoreos_semanales" />
		<searchfield name="descripcion" />
		<listfield name="color" />
		<listfield name="color_html" />
		<listfield name="monitoreos_semanales" />
		<listfield name="descripcion" />
		<action label="ver" target="estado_monitoreo_maint.php?OP=V" operation="V">
			<sendfield classname="cestado_monitoreo" name="id_estado_de_monitoreo"/>
		</action>
		<action label="baja" target="estado_monitoreo_maint_b.php?OP=B" operation="B">
			<sendfield classname="cestado_monitoreo" name="id_estado_de_monitoreo"/>
		</action>
		<action label="modificar" target="estado_monitoreo_maint.php?OP=M" operation="M">
			<sendfield classname="cestado_monitoreo" name="id_estado_de_monitoreo"/>
		</action>
	</build>
</make>

<make object="table_maint"> 
	<build out_file="estado_monitoreo_maint.php" classname="cestado_monitoreo" title="Monitoreo" next="monitoreo.php?last=1&amp;OP=L">
		<field name="id_estado_de_monitoreo" label="Estado" presentation="SEQUENCE" isvisible="false" />
		<field name="color" label="Color" presentation="TEXT" ismandatory="true" />
		<field name="color_html" label="Color Hexa" presentation="TEXT" isvisible="false" />
		<field name="monitoreos_semanales" label="Numero" presentation="TEXT" ismandatory="true" />
		<field name="descripcion" label="Descripcion" presentation="TEXT" ismandatory="true" />
	</build>
</make>

